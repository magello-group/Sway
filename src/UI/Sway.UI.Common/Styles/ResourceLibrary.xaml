<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Merge all resources to one Dictionary  -->
    <!--  Remember that order matters!  -->
    <ResourceDictionary.MergedDictionaries>

        <!--  Load common resources  -->
        <ResourceDictionary Source="/Sway.UI.Common;component/Styles/Colors/AppColors.xaml" />
        <ResourceDictionary Source="/Sway.UI.Common;component/Styles/Icons/AppIcons.xaml" />

        <!--  The Styling Theme  -->
        <ResourceDictionary Source="/Sway.UI.Common;component/Styles/Themes/Titlebar.SwayNorth.xaml" />

    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>